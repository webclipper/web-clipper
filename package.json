{
  "name": "web-clipper",
  "version": "1.8.0",
  "description": "Clip anything to anywhere.",
  "scripts": {
    "commit": "git-cz",
    "dist": "tsc",
    "test": "jest --verbose",
    "dev": "NODE_ENV=development webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern bad.* .",
    "release": "npm run build && node script/release.js",
    "travis": "npm run build && node script/travis_release.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "author": "Diamondyuan",
  "license": "MIT",
  "dependencies": {
    "@diamondyuan/readability": "^0.1.0",
    "@types/showdown": "^1.9.3",
    "@web-clipper/area-selector": "^0.0.3",
    "@web-clipper/chrome-promise": "^0.0.20",
    "@web-clipper/extensions": "^0.0.5",
    "@web-clipper/highlight": "^0.0.20",
    "@web-clipper/message-listener-combiner": "^0.0.5",
    "@web-clipper/shared": "^0.0.20",
    "@web-clipper/turndown": "^0.0.11",
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "codemirror": "^5.47.0",
    "dayjs": "^1.8.14",
    "dva": "2.6.0-beta.12",
    "dva-loading": "^3.0.10",
    "dva-logger": "^1.0.0",
    "dva-model-creator": "^0.3.0",
    "hypermd": "^0.3.11",
    "immutability-helper": "^3.0.1",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "qrcode": "^1.4.1",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^3.1.5",
    "react-markdown": "^4.1.0",
    "regenerator-runtime": "^0.13.3",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@diamondyuan/eslint-config-react-typescript": "^7.0.0",
    "@types/chrome": "^0.0.86",
    "@types/codemirror": "^0.0.76",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/jquery": "^3.3.6",
    "@types/lodash": "^4.14.132",
    "@types/qrcode": "^1.3.3",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-router": "^5.0.1",
    "@types/redux-logger": "^3.0.6",
    "@types/remote-redux-devtools": "^0.5.3",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^3.1.1",
    "compressing": "^1.4.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "less": "^3.8.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "pump": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-import-plugin": "^1.5.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.35.0",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.2",
    "webpack-create-extension-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.2.1"
  },
  "manifest": {
    "manifest_version": 2,
    "icons": {
      "128": "icon.png"
    },
    "browser_action": {
      "default_icon": {
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content_script.js"
        ],
        "run_at": "document_end",
        "all_frames": false
      }
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "web_accessible_resources": [
      "tool.html"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "activeTab",
      "storage",
      "contextMenus",
      "<all_urls>"
    ]
  }
}
