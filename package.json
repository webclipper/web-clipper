{
  "name": "yuque-web-clipper",
  "version": "0.0.8",
  "description": "非官方语雀剪藏软件",
  "scripts": {
    "commit": "git-cz",
    "test": "jest --verbose",
    "dev": "NODE_ENV=development webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern bad.* .",
    "release": "zsh shell/release.zsh"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "author": "Diamondyuan",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.8.4",
    "axios": "^0.18.0",
    "codemirror": "^5.40.2",
    "dayjs": "^1.7.5",
    "hypermd": "^0.3.10",
    "immutability-helper": "^2.9.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.8",
    "qrcode": "^1.3.0",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "readability": "DiamondYuan/readability",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "turndown": "^5.0.1",
    "typescript-fsa": "^3.0.0-beta-2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.72",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.6",
    "@types/lodash": "^4.14.116",
    "@types/qrcode": "^1.3.0",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.12",
    "@types/react-dom": "^16.0.7",
    "babel-eslint": "^9.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^2.10.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-alloy": "^1.4.2",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-typescript": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "chrome": {
    "manifest_version": 2,
    "name": "语雀剪藏",
    "icons": {
      "16": "icons/yuque-16.png",
      "48": "icons/yuque-48.png",
      "128": "icons/yuque-128.png"
    },
    "browser_action": {
      "default_icon": {
        "19": "icons/yuque-19.png",
        "38": "icons/yuque-38.png"
      }
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "js/content_script.js"
        ],
        "run_at": "document_end",
        "all_frames": false
      }
    ],
    "background": {
      "scripts": [
        "js/vendor.js",
        "js/background.js"
      ]
    },
    "web_accessible_resources": [
      "tool.html"
    ],
    "permissions": [
      "cookies",
      "tabs",
      "storage",
      "contextMenus",
      "<all_urls>"
    ]
  }
}
